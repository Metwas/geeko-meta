       "license": "MIT",
       "dependencies": {
              "@geeko/log": "^1.1.0",
              "reflect-metadata": "^0.2.2"
       },
       "devDependencies": {
              "@types/node": "^24.10.4",
              "typescript": "^5.9.3"
       }
}
