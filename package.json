{
       "name": "@geeko/meta",
       "version": "1.2.0",
       "description": "Geeko metadata library for decorators & dependency injection",
       "main": "./dist/main.js",
       "scripts": {
              "build": "rm -rf ./dist && tsc -p ./tsconfig.json",
